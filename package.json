{
  "name": "ffxiv-geobot",
  "version": "0.0.1",
  "description": "FFXIV geolocation guessing bot with leaderboards",
  "main": "index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "build": "tsup src/index.ts --minify",
    "init-tables": "node initTables.js",
    "commands": "tsx src/refreshCommands.ts"
  },
  "keywords": [
    "bot",
    "ffxiv",
    "geo",
    "geolocation",
    "geoguesser",
